<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.tuk.planyourday">

    <application
        android:name=".ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_planner"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_planner_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar">
        <activity
            android:name=".WeekActivity"
            android:exported="true" />
        <activity
            android:name=".AllActivity"
            android:exported="true" />
        <activity
            android:name=".month.MonthActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditTodoActivity" />
    </application>

</manifest>